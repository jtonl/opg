{
    "name": "example/symfony-opa-api",
    "type": "project",
    "require": {
        "symfony/framework-bundle": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/yaml": "^6.4",
        "symfony/routing": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}